{
  "name": "ts-plugin",
  "displayName": "TS Type Plugin",
  "version": "0.1.5",
  "main": "./out/index.js",
  "exports": {
    "require": "./out/index.js",
    "import": "./out/index.mjs"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "check-ts": "node -e \"console.log(require('typescript').version)\""
  },
  "peerDependencies": {
    "typescript": "^5.6.2",
    "@types/ts-expose-internals": "npm:ts-expose-internals@5.6.2"
  }
}
